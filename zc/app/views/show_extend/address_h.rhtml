<body lang=ZH-CN style='tab-interval:21.0pt;text-justify-trim:punctuation'>
<div align="center">
  <button onclick="ww();">生成 Word 文档</button>
</div>
<div class=Section1 style='layout-grid:15.6pt' align="center">
  <%
 shang = @person.length / 2
 lines = @person.length%2 == 0 ? shang : shang + 1
 current = 0
%>
<table id="report" class=MsoNormalTable border=0 cellspacing=0 cellpadding=0 style='border-collapse:collapse;mso-padding-top-alt:0cm;mso-padding-bottom-alt: 0cm;'>
 <% lines.times do %>
  <%
    arbitmaninfo = nil
    if !@person[current].blank?
      arbitmaninfo = TbArbitman.find(:first,:conditions=>" code='#{@person[current].p}' ",:select=>"name,addr,postcode")
    end
  %>
  <tr style='mso-yfti-irow:0;mso-yfti-firstrow:yes;page-break-inside:avoid;height:96.4pt'>
    <td width=361 style='width:270.75pt;padding:0cm .75pt 0cm .75pt;height:96.4pt'>
      <p class=MsoNormal align=center style='margin-top:0cm;margin-right:6.75pt;margin-bottom:0cm;margin-left:6.75pt;margin-bottom:.0001pt;text-align:center'>
        <span  style='font-family:宋体;mso-ascii-font-family:"Times New Roman";mso-hansi-font-family:"Times New Roman"'>
          <%= arbitmaninfo.postcode if !arbitmaninfo.blank? %>
        </span>
      </p>
      <p class=MsoNormal align=center style='margin-top:0cm;margin-right:6.75pt;margin-bottom:0cm;margin-left:6.75pt;margin-bottom:.0001pt;text-align:center'>
        <span  style='font-family:宋体;mso-ascii-font-family:"Times New Roman";mso-hansi-font-family:"Times New Roman"'>
          <%= arbitmaninfo.addr if !arbitmaninfo.blank? %>
        </span>
      </p>
      <p class=MsoNormal align=center style='margin-top:0cm;margin-right:6.75pt; margin-bottom:0cm;margin-left:6.75pt;margin-bottom:.0001pt;text-align:center'>
        <span style='font-family:宋体;mso-ascii-font-family:"Times New Roman";mso-hansi-font-family:"Times New Roman"'>
          <%= arbitmaninfo.name if !arbitmaninfo.blank? %>
        </span>
      </p>
    </td>
    <%
      current = current + 1
      arbitmaninfo = nil
      if !@person[current].blank?
        arbitmaninfo = TbArbitman.find(:first,:conditions=>" code='#{@person[current].p}' ",:select=>"name,addr,postcode")
      end
    %>
    <td width=361 style='width:270.75pt;padding:0cm .75pt 0cm .75pt;height:96.4pt'>
      <p class=MsoNormal align=center style='margin-top:0cm;margin-right:6.75pt;margin-bottom:0cm;margin-left:6.75pt;margin-bottom:.0001pt;text-align:center'>
        <span  style='font-family:宋体;mso-ascii-font-family:"Times New Roman";mso-hansi-font-family:"Times New Roman"'>
          <%= arbitmaninfo.postcode if !arbitmaninfo.blank? %>
        </span>
      </p>
      <p class=MsoNormal align=center style='margin-top:0cm;margin-right:6.75pt;margin-bottom:0cm;margin-left:6.75pt;margin-bottom:.0001pt;text-align:center'>
        <span  style='font-family:宋体;mso-ascii-font-family:"Times New Roman";mso-hansi-font-family:"Times New Roman"'>
          <%= arbitmaninfo.addr if !arbitmaninfo.blank? %>
        </span>
      </p>
      <p class=MsoNormal align=center style='margin-top:0cm;margin-right:6.75pt; margin-bottom:0cm;margin-left:6.75pt;margin-bottom:.0001pt;text-align:center'>
        <span style='font-family:宋体;mso-ascii-font-family:"Times New Roman";mso-hansi-font-family:"Times New Roman"'>
          <%= arbitmaninfo.name if !arbitmaninfo.blank? %>
        </span>
      </p>
    </td>
    <% current = current + 1 %>
   </tr>
 <% end %>
</table>
<p class=MsoNormal><span lang=EN-US><o:p>&nbsp;</o:p></span></p>

</div>
