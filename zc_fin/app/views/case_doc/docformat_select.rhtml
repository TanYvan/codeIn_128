<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
  <head>
    <title>仲裁员选择</title>
    <base   target="_self"/>
    <%= javascript_include_tag :defaults%>
    <link href="/stylesheets/style.css" rel="stylesheet" type="text/css">
    <script language="javascript">
      function r_s(){
        new Ajax.Request('/case_doc/p_set2?spell='+document.getElementById("spell").value, {asynchronous:true, evalScripts:true});
        return false;
      }
    </script>
  </head>
  <body onload="document.getElementById('spell').focus()">

    <div class="title"><span class="cBlue"><div  align="center">格式函列表</div></span></div>
    <%form_tag :url=>{:action=>"docformat_select"},:recevice_code=>params[:recevice_code] do %>
    格式函名称或拼音缩写:<%=text_field_tag(:spell,@spell)%>
    <!--input type="text" id="spell" name="spell" onkeyup="r_s()"-->
      <%=submit_tag "查询" %>
    <%end%>
    <div id='pv_set'>
      <table id="table_1" class="adminlist" cellspacing="1">
        <thead>
          <tr>
            <th nowrap>选择</th>
            <th nowrap>格式函拼音缩写</th>
            <th nowrap>格式函编号</th>
            <th nowrap>格式函名称</th>
          </tr>
        </thead>
        <tbody>
          <%j=0%>
          <%i=1%>
          <%for tb_arbitman in @typ %>
            <tr class="row<%=j % 2%>">
              <td><input type='button' value="选择" onclick="window.returnValue='<%=tb_arbitman.code %>';window.close();"></td>
              <td><%= tb_arbitman.spell%></td>
              <td><%= tb_arbitman.code%></td>
              <td><%= tb_arbitman.name%></td>
            </tr>
            <%i+=1%>
            <%j=j+1%>
          <%end%>
        </tbody>
      </table>
    </div>

  </body>
</html>