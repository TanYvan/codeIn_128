          <%bank_typ=tb_arbitman.bank_typ%>
          <%elsif pp.p_typ=="0002" or pp.p_typ=="0003" or pp.p_typ=="0004"%>
            <%bank_typ=user.bank_typ%>
          <%else%>
          <%end%>
          <%if bank_typ!=nil and bank_typ!=''%>
            <%=@bank_typ[bank_typ]%>
          <%end%>
        </td>
        <td><!-- 银行 -->
          <%if pp.p_typ=="0001"%>
            <%=tb_arbitman.bankname%>
          <%else%>
            <%=user.bankname%>
          <%end%>
        </td>
        <td><!-- 账号 -->
          <%if pp.p_typ=="0001"%>
            <%=tb_arbitman.bankaccount%>
